  "type": "object",
  "properties": {
    "MTDSolutions": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "ScriptName": {
              "type": "string"
            },
            "RelativePath": {
              "type": "string"
            },
            "RunWithPrefix": {
              "type": "string"
            }
